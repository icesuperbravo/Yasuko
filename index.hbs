{{!< default}}

<header id="header"
        class="home-header blog-background banner-mask lazy {{#if
        @blog.cover}}" style="background-image: url({{@blog.cover}}){{else}}no-cover{{/if}}">
    <div class="nav-header-container">
        <a href="{{@blog.url}}" class="svg-logo">
            <span class="svg-logo">
                <img src="./assets/images/Logo.png" alt="product logo" height="60" width="60"></img>
            </span>
        </a>
    </div>
    <div class="header-wrap">
        <div class="home-info-container">
            <a href="{{@blog.url}}"><h2>TeamArtizan</h2></a>
            <h4>打造雇主品牌，助力企业成长</h4>
        </div>
        <div class="arrow_down" data-offset="-45">
            <a href="javascript:;"></a>
        </div>
    </div>
</header>
{{! The main content area on the homepage }}
<main id="main" class="content homepage" role="main">
    {{! Each post will be output using this markup }}
    {{> "loop"}}
    {{!! After all the posts, we have the previous/next pagination links }}

    {{!pagination}}
</main>


